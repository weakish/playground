{
    "compilerOptions": {
        "strict": true,
        "moduleResolution": "Node",
        "noUnusedParameters": true,
        "noUnusedLocals": true,
	    "noImplicitReturns": true,
	    "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true,
        "allowJs": true,
        "checkJs": true,
        "target": "es2017",
        "incremental": true,
    }
}